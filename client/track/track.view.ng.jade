h1 Track!
div(data-ng-init="jinglesFactory.getSingleTrack(track)")
  h2 {{singleTrack.title}}

  div(plangular="{{playURL}}" data-ng-if="playURL")
    {{playURL}}
    h3 {{track.title}}
    p {{ currentTime | hhmmss }} | {{ duration | hhmmss }}
    div(progress ng-value="currentTime / duration || 0")
      {{ currentTime / duration || 0 }}
    button(ng-click="playPause()") play/pause